<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <runtime>
        <gcConcurrent enabled="true"/>
    </runtime>
    <nunit>
        <settings>
            <setting name="DefaultTestCaseTimeout" value="30000"/>
            <setting name="NumberOfTestWorkers" value="4"/>
        </settings>
        <test-runner>netcore</test-runner>
        <test-suites>
            <test-suite>
                <exclude>
                    <category>Create</category>
                    <category>Delete</category>
                    <category>Index</category>
                </exclude>
            </test-suite>
        </test-suites>
    </nunit>
</configuration>
