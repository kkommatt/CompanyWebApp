@model List<string>


    <h1>Країни, всі громадяни яких є програмістами через множинне порівняння</h1>
<h4>
    SELECT DISTINCT co.Name AS CountryName 
    FROM Countries co 
    WHERE NOT EXISTS ( 
        SELECT * 
        FROM Citizens ct 
        WHERE ct.CountryId = co.Id 
            AND ct.Id NOT IN ( 
                SELECT p.CitizenId 
                FROM Programmers p 
            ) 
    );
</h4>
    @if (Model.Count > 0)
    {
        <ul>
            @foreach (var country in Model)
            {
                <li>@country</li>
            }
        </ul>
    }
    else
    {
        <p>No countries found</p>
    }
